[declarations]

[include]

[ignore]
; vim session undo history
.*/\.undodir/.*

; intentionally bogus JSON file
.*\/multi-semantic-release\/test\/fixtures\/invalidPackage.json

[untyped]

[libs]

[lints]

[options]
emoji=true
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
esproposal.decorators=warn
esproposal.export_star_as=warn
esproposal.optional_chaining=enable
esproposal.nullish_coalescing=enable
experimental.const_params=true
lazy_mode=none
log.file=./flow.log
max_header_tokens=10
module.file_ext=.js
module.file_ext=.json
module.ignore_non_literal_requires=false
module.resolver=<PROJECT_ROOT>/.pnp.sh
module.system=haste
module.use_strict=false
munge_underscores=false
no_flowlib=false
;server.max_workers=
;sharedmemory.dirs=/dev/shm
;sharedmemory.dirs=/tmp
;sharedmemory.minimum_available=536870912
;sharedmemory.dep_table_pow=17
;sharedmemory.hash_table_pow=19
;sharedmemory.heap_size=26843545600
;sharedmemory.log_level=0
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
suppress_type=$FlowFixMe
suppress_type=$FlowIgnore
;temp_dir=/tmp/flow
;traces=0

[version]
0.101.0
