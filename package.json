{
  "name": "@meatwallace/foundations",
  "version": "1.0.0",
  "description": "presets, configurations & utilities used in my projects",
  "author": "Geoff Whatley <me@geoffwhatley.com> (https://geoffwhatley.com/)",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/meatwallace/foundations#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/meatwallace/foundations.git"
  },
  "bugs": {
    "url": "https://github.com/meatwallace/foundations/issues"
  },
  "scripts": {
    "commit": "commit",
    "lint": "prettier-eslint",
    "lint:ci": "eslint --resolve-plugins-relative-to packages/eslint-config-base **/*.js",
    "lint:fix": "prettier-eslint --write",
    "test": "jest --watch",
    "test:ci": "jest --bail",
    "typecheck": "flow",
    "release": "multi-semantic-release --execasync",
    "update-types": "npx flow-typed install --skip"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-flow": "7.0.0",
    "@commitlint/cli": "8.0.0",
    "@commitlint/prompt-cli": "8.0.0",
    "@meatwallace/commitlint-config": "workspace:*",
    "@meatwallace/eslint-config-base": "workspace:*",
    "@meatwallace/prettier-config": "workspace:*",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/commit-analyzer": "6.2.0",
    "@semantic-release/git": "7.0.12",
    "@semantic-release/github": "5.4.0",
    "@semantic-release/release-notes-generator": "7.2.0",
    "@types/chalk": "2.2.0",
    "babel-jest": "24.8.0",
    "chalk": "2.4.2",
    "cosmiconfig": "5.2.1",
    "eslint": "6.0.1",
    "flow-bin": "0.102.0-rc",
    "husky": "3.0.0",
    "indent-string": "4.0.0",
    "jest": "24.8.0",
    "lint-staged": "9.0.2",
    "multi-semantic-release": "1.1.0",
    "prettier-eslint-cli": "5.0.0",
    "require-in-the-middle": "4.0.0",
    "rxjs": "^6.5.2",
    "semantic-release": "15.13.16"
  },
  "resolutions": {
    "@semantic-release/npm": "github:meatwallace/npm#3aa2e61"
  },
  "// tooling configuration": "",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@semantic-release/npm": "github:meatwallace/npm#3aa2e61"
  }
}
