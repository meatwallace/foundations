{
  "name": "@meatwallace/foundations",
  "version": "1.0.0",
  "description": "presets, configurations & utilities used in my projects",
  "author": "Geoff Whatley <me@geoffwhatley.com> (https://geoffwhatley.com/)",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/meatwallace/foundations#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/meatwallace/foundations.git"
  },
  "bugs": {
    "url": "https://github.com/meatwallace/foundations/issues"
  },
  "scripts": {
    "commit": "commit",
    "lint": "prettier-eslint",
    "lint:ci": "eslint **/*.js",
    "lint:fix": "prettier-eslint --write",
    "test": "jest --watch",
    "test:ci": "jest --bail",
    "typecheck": "flow",
    "release": "multi-semantic-release --execasync",
    "update-types": "npx flow-typed install --skip"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-flow": "7.0.0",
    "@commitlint/cli": "8.0.0",
    "@commitlint/prompt-cli": "8.0.0",
    "@meatwallace/commitlint-config": "*",
    "@meatwallace/eslint-config-base": "*",
    "@meatwallace/prettier-config": "*",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/commit-analyzer": "6.1.0",
    "@semantic-release/git": "7.0.8",
    "@semantic-release/github": "5.4.0",
    "@semantic-release/npm": "5.1.7",
    "@semantic-release/release-notes-generator": "7.1.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "chalk": "2.4.2",
    "cosmiconfig": "5.2.1",
    "eslint": "5.16.0",
    "flow-bin": "0.101.0",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "lint-staged": "8.2.0",
    "multi-semantic-release": "1.1.0",
    "prettier-eslint-cli": "4.7.1",
    "require-in-the-middle": "4.0.0",
    "semantic-release": "15.13.12"
  },
  "// tooling configuration": "",
  "workspaces": [
    "packages/*"
  ],
  "installConfig": {
    "pnp": true
  }
}
